<h1>Listing other_dates</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @other_dates.each do |other_date| %>
      <tr>
        <td><%= other_date.date %></td>
        <td><%= other_date.description %></td>
        <td><%= link_to 'Show', other_date %></td>
        <td><%= link_to 'Edit', edit_other_date_path(other_date) %></td>
        <td><%= link_to 'Destroy', other_date, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Other date', new_other_date_path %>
